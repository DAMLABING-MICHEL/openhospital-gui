	<parameter name="fromdate" isForPrompting="true" class="java.lang.String"/>
	<parameter name="todate" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT ADM_ID, 
		ADM_PAT_ID, 
		DATE(ADM_DATE_DIS) AS DATE_A, 
		YEAR(ADM_DATE_DIS) AS YEAR_A, 
		MONTH(ADM_DATE_DIS) AS MONTH_A, 
		WRD_NAME, 
		IF(TIMESTAMPDIFF(YEAR, PAT_BDATE, ADM_DATE_ADM)<=10,"UNDER TEN","OVER TEN") AS AGE, 
		ROUND(SUM(BLL_AMOUNT),2) AS BILL, 
		ROUND(SUM(BLL_AMOUNT) - SUM(BLP_AMOUNT),2) AS FREE,
		ROUND(SUM(BLP_AMOUNT),2) AS NET
FROM ADMISSION INNER JOIN PATIENT ON ADM_PAT_ID = PAT_ID
 JOIN WARD ON WRD_ID_A = ADM_WRD_ID_A
 JOIN (
 SELECT BLL_ID, BLL_DATE, BLL_ID_PAT, BLL_AMOUNT, SUM(BLP_AMOUNT) AS BLP_AMOUNT, BLL_STATUS FROM BILLS JOIN BILLPAYMENTS ON BLL_ID = BLP_ID_BILL
 GROUP BY BLL_ID
 ) AS BILLS ON BLL_ID_PAT = PAT_ID
WHERE ADM_DELETED = 'N'
  AND BLL_STATUS = 'C'
  AND PAT_DELETED = 'N'
  AND ADM_DATE_DIS IS NOT NULL
  AND DATE(ADM_DATE_DIS) BETWEEN STR_TO_DATE($P{fromdate}, '%d/%m/%Y') AND STR_TO_DATE($P{todate}, '%d/%m/%Y')
  AND BLL_DATE BETWEEN ADM_DATE_ADM AND ADM_DATE_DIS
GROUP BY ADM_ID
ORDER BY YEAR_A, MONTH_A, WRD_NAME, AGE]]></queryString>

