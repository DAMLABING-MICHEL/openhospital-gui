	<queryString><![CDATA[SELECT 
	GROUP_ID,
	GROUP_CODE,
    item AS ITEM,
    SUM(quantity) as QUANTITY,
    SUM(amount) as AMOUNT,
	SUM(free) as FREE,
    SUM(free_amount) as FREE_AMOUNT,
    SUM(staff) as STAFF,
    SUM(staff_amount) as STAFF_AMOUNT,
    SUM(exempted) as EXEMPTED,
    SUM(exempt_amount) as EXEMPTED_AMOUNT,
	SUM(amount) + SUM(free_amount) + SUM(staff_amount) + SUM(exempt_amount) AS INCOME
FROM
    (SELECT  1 as GROUP_ID, 'PRESCRIPTIONS' as GROUP_CODE,
            BLI_ITEM_DESC AS item,
            SUM(BLI_QTY) AS quantity,
            SUM(BLI_ITEM_AMOUNT * BLI_QTY) AS amount,
            SUM(IF(EXISTS(select 1 
				FROM BILLITEMS 
				WHERE BLI_ID_BILL = BLL_ID
					AND BLI_ITEM_DESC like 'FREE%'), BLI_QTY, 0)) as free,
            SUM(IF(EXISTS( select 1
                FROM BILLITEMS
                WHERE BLI_ID_BILL = BLL_ID
					AND BLI_ITEM_DESC like 'FREE%'), (0 - BLI_ITEM_AMOUNT * BLI_QTY), 0)) AS free_amount,
			SUM(IF(EXISTS(select 1 
				FROM BILLITEMS 
				WHERE BLI_ID_BILL = BLL_ID
					AND BLI_ITEM_DESC like 'STAFF%'), BLI_QTY, 0)) as staff,
            SUM(IF(EXISTS( select 1
                FROM BILLITEMS
                WHERE BLI_ID_BILL = BLL_ID
					AND BLI_ITEM_DESC like 'STAFF%'), (0 - BLI_ITEM_AMOUNT * BLI_QTY), 0)) AS staff_amount,
            SUM(IF(EXISTS( select 1
                FROM BILLITEMS
                WHERE BLI_ID_BILL = BLL_ID
					AND BLI_ITEM_DESC = 'EXEMPT'), BLI_QTY, 0)) AS exempted,
            SUM(IF(EXISTS(select 1
                FROM BILLITEMS
                WHERE BLI_ID_BILL = BLL_ID
					AND BLI_ITEM_DESC = 'EXEMPT'), (0 - BLI_ITEM_AMOUNT * BLI_QTY), 0)) AS exempt_amount
    FROM MEDICALDSRWARDPRESCRIPTION
    INNER JOIN BILLS ON MWP_BLL_ID = BLL_ID
    INNER JOIN BILLITEMS ON BLI_ID_BILL = BLL_ID
    WHERE BLI_ITEM_AMOUNT >= 0
		AND BLL_STATUS = 'C'
		AND BLI_ID_PRICE like 'MED%'
		AND DATE(BLL_UPDATE) BETWEEN STR_TO_DATE($P{fromdate}, '%d/%m/%Y') AND STR_TO_DATE($P{todate}, '%d/%m/%Y')
    GROUP BY BLI_ITEM_DESC 
	
	UNION ALL 
	
	SELECT  2 as GROUP_ID, 'EMERGENCY' as GROUP_CODE, BLI_ITEM_DESC AS item,
            SUM(BLI_QTY) AS quantity,
            SUM(BLI_ITEM_AMOUNT * BLI_QTY) AS amount,
            0 as free,
            0 AS free_amount,
			0 as staff,
            0 AS staff_amount,
            0 as exempted,
            0 as exempt_amount
    FROM BILLITEMS
    JOIN BILLS ON BLL_ID = BLI_ID_BILL
    WHERE BLI_ITEM_DESC LIKE 'Emer%'
		AND BLL_STATUS = 'C'
		AND DATE(BLL_UPDATE) BETWEEN STR_TO_DATE($P{fromdate}, '%d/%m/%Y') AND STR_TO_DATE($P{todate}, '%d/%m/%Y')
    GROUP BY BLI_ITEM_DESC) as bill_items
GROUP BY item
ORDER BY GROUP_ID, item ASC;]]></queryString>

