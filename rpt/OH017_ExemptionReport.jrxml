	<queryString><![CDATA[SELECT 
    'FREE' AS TYPE,
    BLL_UPDATE,
    PAT_ID,
    PAT_PCODE,
    PAT_NAME,
    PAT_CITY,
    BLL_ID,
    BLL_DATE,
    IF(EXISTS( SELECT 
                1
            FROM
                ADMISSION
            WHERE ADM_PAT_ID = PAT_ID AND
                DATE(BLL_DATE) BETWEEN DATE(ADM_DATE_ADM) AND DATE(ADM_DATE_DIS)),
        BLL_AMOUNT,
        0) AS 'IPD_AMOUNT',
    IF(NOT EXISTS( SELECT 
                1
            FROM
                ADMISSION
            WHERE ADM_PAT_ID = PAT_ID AND
                DATE(BLL_DATE) BETWEEN DATE(ADM_DATE_ADM) AND DATE(ADM_DATE_DIS)),
        BLL_AMOUNT,
        0) AS 'OPD_AMOUNT',
    SUM(BLI_ITEM_AMOUNT) AS BLL_FREE
FROM
    PATIENT
        JOIN
    BILLS ON PAT_ID = BLL_ID_PAT
        JOIN
    BILLITEMS ON BLL_ID = BLI_ID_BILL
WHERE
    BLI_ITEM_DESC LIKE 'FREE%'
        AND BLL_STATUS = 'C'
        AND DATE(BLL_UPDATE) BETWEEN STR_TO_DATE($P{fromdate}, '%d/%m/%Y') AND STR_TO_DATE($P{todate}, '%d/%m/%Y')
GROUP BY PAT_ID, BLL_ID , BLL_DATE UNION SELECT 
    'STAFF' AS TYPE,
    BLL_UPDATE,
    PAT_ID,
    PAT_PCODE,
    PAT_NAME,
    PAT_CITY,
    BLL_ID,
    BLL_DATE,
    IF(EXISTS( SELECT 
                1
            FROM
                ADMISSION
            WHERE ADM_PAT_ID = PAT_ID AND
                DATE(BLL_DATE) BETWEEN DATE(ADM_DATE_ADM) AND DATE(ADM_DATE_DIS)),
        BLL_AMOUNT,
        0) AS 'IPD_AMOUNT',
    IF(NOT EXISTS( SELECT 
                1
            FROM
                ADMISSION
            WHERE ADM_PAT_ID = PAT_ID AND
                DATE(BLL_DATE) BETWEEN DATE(ADM_DATE_ADM) AND DATE(ADM_DATE_DIS)),
        BLL_AMOUNT,
        0) AS 'OPD_AMOUNT',
    SUM(BLI_ITEM_AMOUNT) AS BLL_FREE
FROM
    PATIENT
        JOIN
    BILLS ON PAT_ID = BLL_ID_PAT
        JOIN
    BILLITEMS ON BLL_ID = BLI_ID_BILL
WHERE
    BLI_ITEM_DESC LIKE 'STAFF%'
        AND BLL_STATUS = 'C'
        AND DATE(BLL_UPDATE) BETWEEN STR_TO_DATE($P{fromdate}, '%d/%m/%Y') AND STR_TO_DATE($P{todate}, '%d/%m/%Y')
GROUP BY PAT_ID, BLL_ID , BLL_DATE UNION SELECT 
    'EXEMPT' AS TYPE,
    BLL_UPDATE,
    PAT_ID,
    PAT_PCODE,
    PAT_NAME,
    PAT_CITY,
    BLL_ID,
    BLL_DATE,
    IF(EXISTS( SELECT 
                1
            FROM
                ADMISSION
            WHERE ADM_PAT_ID = PAT_ID AND
                DATE(BLL_DATE) BETWEEN DATE(ADM_DATE_ADM) AND DATE(ADM_DATE_DIS)),
        BLL_AMOUNT,
        0) AS 'IPD_AMOUNT',
    IF(NOT EXISTS( SELECT 
                1
            FROM
                ADMISSION
            WHERE ADM_PAT_ID = PAT_ID AND
                DATE(BLL_DATE) BETWEEN DATE(ADM_DATE_ADM) AND DATE(ADM_DATE_DIS)),
        BLL_AMOUNT,
        0) AS 'OPD_AMOUNT',
    SUM(BLI_ITEM_AMOUNT) AS BLL_FREE
FROM
    PATIENT
        JOIN
    BILLS ON PAT_ID = BLL_ID_PAT
        JOIN
    BILLITEMS ON BLL_ID = BLI_ID_BILL
WHERE
    BLI_ITEM_DESC LIKE 'EXE%'
        AND BLL_STATUS = 'C'
        AND DATE(BLL_UPDATE) BETWEEN STR_TO_DATE($P{fromdate}, '%d/%m/%Y') AND STR_TO_DATE($P{todate}, '%d/%m/%Y')
GROUP BY PAT_ID, BLL_ID , BLL_DATE;]]></queryString>

