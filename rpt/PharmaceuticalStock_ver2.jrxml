	<queryString><![CDATA[SELECT "IN STOCK" AS STATUS, MEDICALS.* FROM (
SELECT MDSR_ID, MDSRT_DESC, MDSR_CODE, MDSR_DESC, MDSR_IN_QTI - MDSR_OUT_QTI AS STOCK, AVG_COST, AVG_COST * (MDSR_IN_QTI - MDSR_OUT_QTI) AS TOTAL
FROM MEDICALDSR
JOIN MEDICALDSRTYPE ON MDSR_MDSRT_ID_A = MDSRT_ID_A
JOIN 
(SELECT MDC_MDSR_ID, AVG(MDC_COST) AS AVG_COST
FROM MEDICALDSRCOST
GROUP BY MDC_MDSR_ID) AS COSTS
ON MDSR_ID = MDC_MDSR_ID
ORDER BY MDSRT_DESC, MDSR_DESC) AS MEDICALS
WHERE STOCK > 0

UNION ALL

SELECT "OUT OF STOCK" AS STATUS, MEDICALS.* FROM (
SELECT MDSR_ID, MDSRT_DESC, MDSR_CODE, MDSR_DESC, MDSR_IN_QTI - MDSR_OUT_QTI AS STOCK, AVG_COST, AVG_COST * (MDSR_IN_QTI - MDSR_OUT_QTI) AS TOTAL
FROM MEDICALDSR
JOIN MEDICALDSRTYPE ON MDSR_MDSRT_ID_A = MDSRT_ID_A
JOIN 
(SELECT MDC_MDSR_ID, AVG(MDC_COST) AS AVG_COST
FROM MEDICALDSRCOST
GROUP BY MDC_MDSR_ID) AS COSTS
ON MDSR_ID = MDC_MDSR_ID
ORDER BY MDSRT_DESC, MDSR_DESC) AS MEDICALS
WHERE STOCK = 0]]></queryString>

